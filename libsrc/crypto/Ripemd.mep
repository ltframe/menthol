import MCryptography;
use CRIPEMD;
module Ripemd
{
	var const @cripemdinst =mmrt(CRIPEMD);
	def RIPEMD128:$arg1
	{
		return @cripemdinst.RIPEMD128($arg1);
	}

	def RIPEMD160:$arg1
	{
		return @cripemdinst.RIPEMD160($arg1);
	}
	def RIPEMD256:$arg1
	{
		return @cripemdinst.RIPEMD256($arg1);
	}
	def RIPEMD320:$arg1
	{
		return @cripemdinst.RIPEMD320($arg1);
	}	
}


