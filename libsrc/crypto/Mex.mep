import MCryptography;
import "FileSystem"
use CMDX;
use File;
module Mdx
{
	def  MD5:$arg1
	{
		return CMDX::MD5($arg1);
	}
	def  MD4:$arg1
	{
		return CMDX::MD4($arg1);
	}
	def  MD2:$arg1
	{
		return CMDX::MD2($arg1);
	}

	def MD5File:$arg1
	{
		var file = File.Open($arg1);
		var str = File.Readfile(file);
		File.Close();
		return str;
	}

}


