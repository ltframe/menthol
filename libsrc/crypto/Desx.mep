import MCryptography;
import String;
use CDES,String;
module Desx
{
	def  DESEncryp:$arg1,$arg2
	{
		if(String.Length($arg2)!=16)
		{
			return -1;
		}
		return CDES.DESEncryp($arg1,$arg2);
	}
	def  DESDecryp:$arg1,$arg2
	{
		if(String.Length($arg2)!=16)
		{
			return -1;
		}
		return CDES.DESDecryp($arg1,$arg2);
	}

	
}


