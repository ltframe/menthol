import miup;
use CIup;
module IupHandle
{
	var const @ciupinst =mmrt(CIup);
	def SetHandle:$arg1,$arg2
	{
		return @ciupinst.SetHandle($arg1,$arg2);
	}

	def GetHandle:$arg1
	{
		return @ciupinst.GetHandle($arg1);
	}

	def GetName:$arg1
	{
		return @ciupinst.GetName($arg1);
	}

	def GetAllNames:$arg1
	{
		return @ciupinst.GetAllNames($arg1);
	}
	def GetAllDialogs:$arg1
	{
		return @ciupinst.GetAllDialogs($arg1);
	}
}


