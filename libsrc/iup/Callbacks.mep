import miup;
use CIup;
module IupCallbacks
{
	var const @ciupinst =mmrt(CIup);
	def MainLoop:
	{
		return @ciupinst.MainLoop();
	}

	def MainLoopLevel:
	{
		return @ciupinst.MainLoopLevel();
	}

	def LoopStep:
	{
		return @ciupinst.LoopStep();
	}

	def ExitLoop:
	{
		return @ciupinst.ExitLoop();
	}

	def Flush:
	{
		return @ciupinst.Flush();
	}
	def SetCallback:$arg1,$arg2,$arg3
	{
		return @ciupinst.SetCallback($arg1,$arg2,$arg3);
	}	
}


