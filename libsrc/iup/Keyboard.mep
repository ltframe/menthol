import miup;
use CIup;
module IupKeyboard
{
	var const @ciupinst =mmrt(CIup);
	def NextField:$arg1
	{
		return @ciupinst.NextField($arg1);
	}

	def PreviousField:$arg1
	{
		return @ciupinst.PreviousField($arg1);
	}

	def GetFocus:
	{
		return @ciupinst.GetFocus();
	}

	def SetFocus:$arg1
	{
		return @ciupinst.SetFocus($arg1);
	}

	def GetKeyCodeString:$arg1
	{
		return @ciupinst.GetKeyCodeString($arg1);
	}
}


