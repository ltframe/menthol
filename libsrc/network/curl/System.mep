import mlibcurl;
use CCurl_System;
module CurlSystem
{
	var const @inst = mmrt(CCurl_System);
	def init:
	{
		return @inst.init();
	}

	def cleanup:$arg1
	{
		return @inst.cleanup($arg1);
	}

	def perform:$arg1
	{
		return @inst.perform($arg1);
	}

	def version:
	{
		return @inst.version();
	}

	def unescape:$arg1,$arg2
	{
		return @inst.unescape($arg1,$arg2);
	}

	def escape:$arg1,$arg2
	{
		return @inst.escape($arg1,$arg2);
	}
}